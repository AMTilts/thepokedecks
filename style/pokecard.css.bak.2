.div-buttons {
  /* display: block; */
  flex-direction: row;
  flex-wrap: nowrap;
  align-items: space-between;
  align-self: center;
  align-content: space-between;
  text-align: center;
  justify-content: center;
  width: 100%;
  height: 30px;
}

.button-next {
  background-color: rgb(58, 82, 117);
  color: rgb(206, 206, 206);
  border-style: solid;
  padding: 1px 6px;
  width: 125px;
  height: 100%;
  /* min-height: 30px; */
  border-width: 2px 2px 5px 5px;
  border-color: rgb(7, 15, 51);
  text-decoration: bold;
  border-radius: 4px 4px 4px 4px;
  font-family: 'PT Sans Narrow', sans-serif;
  text-transform: uppercase;
  font-size: 0.8rem;
  margin-left: 5px;
}

.button-next:hover {
  text-align: center;
  justify-content: center;
  background-color: rgb(21, 49, 100);
  background-blend-mode: color-burn;
  color: white;
  box-shadow: rgb(55, 55, 55) 1px 1px;
  border-width: 3px 3px 3px 3px;
  border-color: rgb(87, 119, 168);
}

.button-prev {
  background-color: rgb(197, 115, 115);
  color: rgb(48, 48, 48);
  border-style: solid;
  border-width: 2px 2px 5px 5px;
  border-color: rgb(112, 4, 0);
  width: 125px;
  height: 100%;
  max-height: 35px;
  border-radius: 4px 4px 4px 4px;
  font-family: 'PT Sans Narrow', sans-serif;
  text-transform: uppercase;
  font-size: 0.8rem;
}

.button-prev:hover {
  background-color: rgb(1, 25, 48);
  color: white;
}

.nav-link {
  color: white;
  text-transform: uppercase;
  font-family: 'PT Sans Narrow', sans-serif;
}

